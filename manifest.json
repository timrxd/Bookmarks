{
  "manifest_version": 2,

  "name": "Bookmarks Extension",
  "description": "List of easy to update bookmarks.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "bookmark.png",
    "default_popup": "bookmark.html"
  },
  "permissions": [
    "activeTab",
    "storage"
],
"content_scripts": [ {
  "js": [ "jquery-3.0.0.min.js" ],
   "matches": ["<all_urls>"]
}]
}
